{
  "name": "prisma-on-render",
  "version": "1.0.0",
  "type": "module",
  "description": "Example Express API that uses Prisma ORM, deployed on Render",
  "main": "index.js",
  "scripts": {
    "build": "prisma generate",
    "dev": "node --env-file=.env --watch src/index.js",
    "start": "npm run dev",
    "dev:noredis": "set DISABLE_REDIS=true && node --env-file=.env --watch src/index.js",
    "start:noredis": "set DISABLE_REDIS=true && node --env-file=.env src/index.js"
  },
  "author": "@jess-render",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.958.0",
    "@prisma/adapter-pg": "^6.9.0",
    "@prisma/client": "^6.9.0",
    "axios": "^1.6.7",
    "body-parser": "^2.2.1",
    "bullmq": "^5.66.2",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.2.1",
    "ioredis": "^5.8.2",
    "morgan": "^1.10.1",
    "node-fetch": "^3.3.2",
    "socket.io": "^4.8.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "vscode-languageserver": "^9.0.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "prisma": "^6.19.2"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
